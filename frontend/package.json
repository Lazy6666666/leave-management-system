{
  "name": "leave-management-frontend",
  "version": "1.0.0",
  "description": "Frontend for the leave management system",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "tsc -p e2e/tsconfig.json && playwright test",
    "test:e2e:ui": "tsc -p e2e/tsconfig.json && playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "tsc -p e2e/tsconfig.json && playwright test visual-regression",
    "test:visual:update": "tsc -p e2e/tsconfig.json && playwright test visual-regression --update-snapshots",
    "test:visual:ui": "tsc -p e2e/tsconfig.json && playwright test visual-regression --ui",
    "test:visual:report": "playwright show-report",
    "test:a11y": "tsc -p e2e/tsconfig.json && playwright test accessibility",
    "test:a11y:enhanced": "tsc -p e2e/tsconfig.json && playwright test accessibility-enhanced",
    "test:a11y:keyboard": "tsc -p e2e/tsconfig.json && playwright test keyboard-navigation",
    "test:a11y:all": "tsc -p e2e/tsconfig.json && playwright test accessibility accessibility-enhanced keyboard-navigation",
    "prepare": "husky install || true",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "next lint --quiet",
    "check-all": "run-s check-format check-lint check-types build",
    "validate-typescript": "tsx scripts/validate-typescript.ts",
    "production-check": "tsx scripts/production-readiness-check.ts",
    "cleanup:validate": "run-s validate-typescript production-check",
    "cleanup:files": "tsx scripts/cleanup-files.ts",
    "cleanup:files:dry": "tsx scripts/cleanup-files.ts --dry-run --verbose",
    "cleanup:files:stats": "tsx scripts/cleanup-files.ts --stats",
    "test:all": "tsx scripts/run-all-tests.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "next lint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "@testing-library/jest-dom": "^6.9.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "file-saver": "^2.0.5",
    "lucide-react": "^0.544.0",
    "mini-svg-data-uri": "^1.4.4",
    "motion": "^12.23.22",
    "next": "^15.5.4",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.63.0",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "xlsx": "^0.18.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.55.1",
    "@storybook/addon-essentials": "^8.2.0",
    "@storybook/addon-interactions": "^8.2.0",
    "@storybook/addon-links": "^8.2.0",
    "@storybook/addon-onboarding": "^8.2.0",
    "@storybook/blocks": "^8.2.0",
    "@storybook/nextjs": "^8.2.0",
    "@storybook/react": "^8.2.0",
    "@storybook/test": "^8.2.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.19.19",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "glob": "^10.4.5",
    "husky": "^9.1.0",
    "jsdom": "^27.0.0",
    "node-mocks-http": "^1.17.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "storybook": "^8.2.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.20.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
